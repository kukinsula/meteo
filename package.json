{
  "name": "meteo",
  "version": "1.0.0",
  "description": "Meteo database updater",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "init": "tsc && node ./dist/app.js --config config/config.json --mode init",
    "update": "tsc && node ./dist/app.js --config config/config.json --mode update"
  },
  "keywords": [
    "meteo"
  ],
  "author": "TEXIER-ATGER Lucas",
  "license": "ISC",
  "dependencies": {
    "argparse": "^1.0.9",
    "iconv-lite": "^0.4.19",
    "jsdom": "^11.5.1",
    "mongoose": "^4.13.7",
    "nodemailer": "^4.4.1",
    "nunjucks": "^3.0.1",
    "source-map-support": "^0.5.0",
    "tabletojson": "^0.6.0",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "@types/argparse": "^1.0.33",
    "@types/jsdom": "^11.0.4",
    "@types/mongoose": "^4.7.30",
    "@types/node": "^8.5.2",
    "@types/nodemailer": "^4.3.1",
    "@types/nunjucks": "^3.0.0",
    "@types/winston": "^2.3.7"
  }
}
